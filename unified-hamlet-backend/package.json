{
  "name": "unified-hamlet-backend",
  "version": "1.0.0",
  "description": "Unified Iraqi Election Platform Backend - 7,769 Candidates + Social Features",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx prisma/seed.ts",
    "import:candidates": "tsx scripts/import-candidates.ts",
    "import:test": "tsx scripts/import-candidates.ts --limit 100",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "election",
    "iraq",
    "candidates",
    "api",
    "express",
    "prisma",
    "postgresql"
  ],
  "author": "Hamlet Platform Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "axios": "^1.7.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "csv-parser": "^3.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.2.5",
    "prisma": "^5.18.0",
    "tsx": "^4.19.0",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
